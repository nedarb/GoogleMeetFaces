{
  "name": "GoogleMeetFaces",
  "version": "0.2.3",
  "description": "Chrome extension to track Google meet video stream using picture-in-picture",
  "scripts": {
    "build": "npm run sync:version && webpack --config webpack.prod.js",
    "dev": "npm run sync:version && webpack --config webpack.dev.js",
    "watch": "webpack -w --config webpack.dev.js",
    "package": "yarn dev && zip -r dist.zip dist",
    "sync:version": "node scripts/syncVersion.js"
  },
  "engines": {
    "node": ">=12 <16.0"
  },
  "author": "Braden Hays",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.60",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "css-loader": "^3.4.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.0.1",
    "typescript": "^3.8.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "canvas-txt": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webextension-polyfill-ts": "^0.22.0"
  }
}
